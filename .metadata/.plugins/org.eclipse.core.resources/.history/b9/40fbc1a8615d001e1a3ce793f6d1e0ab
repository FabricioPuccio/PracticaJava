package principal;

import ejercicios.NumeroCapicua;

public class Main {

	public static void main(String[] args) {
		System.out.println(NumeroCapicua.numeroCapicua(null));
		NumeroCapicua.numeroCapicua(null); //false
		NumeroCapicua.numeroCapicua(123454321);//true
		NumeroCapicua.numeroCapicua(-123454321);//true
		NumeroCapicua.numeroCapicua(2);//true
		NumeroCapicua.numeroCapicua(0);//true
		NumeroCapicua.numeroCapicua(231);//false
		NumeroCapicua.numeroCapicua(123);//false
		
	}

}
